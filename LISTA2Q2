#include <stdio.h>
 
int main() {
 
    double A, B, C, D;
    
    scanf("%lf",&A);
    scanf("%lf",&B);
    scanf("%lf",&C);
    
   D = (B*B)-4*A*C;
   
   if (D>=0 &&A!=0){
      printf("R1 = %lf\n", ((B*-1) + sqrt(D)) / (2*A));
      printf("R2 = %lf\n", ((B*-1) - sqrt(D)) / (2*A));
   }
   
   {
       printf("impossivel calcular\n");
   }
    
    return 0;
}
