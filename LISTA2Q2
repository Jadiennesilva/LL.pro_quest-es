#include <stdio.h>
#include<math.h>

 int main() {
 
    double A, B, C, DELTA;
    
    scanf("%lf",&A);
    scanf("%lf",&B);
    scanf("%lf",&C);
    
   DELTA = (B*B)-4*A*C;
   
   if (DELTA >=0 && A!=0){
      printf("R1 = %lf\n", ((B*-1) + sqrt(DELTA)) / (2*A));
      printf("R2 = %lf\n", ((B*-1) - sqrt(DELTA)) / (2*A));
   }
   
   {
       printf("impossivel calcular\n");
   }
    
    return 0;
}

