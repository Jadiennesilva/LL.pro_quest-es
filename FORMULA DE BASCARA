#include <stdio.h>
#include<math.h>

 int main() {
 
    double A, B, C;
    
    scanf("%lf",&A);
    scanf("%lf",&B);
    scanf("%lf",&C);
    
   if (((B*B) - 4 * A *C <0) || (A == 0)){
       
       printf("impossivel calcular\n");
   }
   
   {
    printf("R1 = %lf\n", (-B + sqrt(B * B -4 * A *C)) / (2*A));
    printf("R2 = %lf\n", (-B - sqrt(B * B -4 * A *C)) / (2*A));
   }
   
  
    
    return 0;
}



quetao fuso horario


#include <stdio.h>
 
int main() {
 
  int S, T, F, HORA;
  
  scanf("%d",&S);
  scanf("%d",&T);
  scanf("%d",&F);
  
  HORA = S + T + + F;
   if( HORA>= 24 ) HORA -= 24{
       (HORA < 0 ) HORA += 24
   }
   
  {
      printf(hora);
  }
  
 
    return 0;
}
