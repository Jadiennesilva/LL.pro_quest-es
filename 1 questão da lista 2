#include <stdio.h>
 
int main() {
    
    int A, B, C, D;
    
    scanf("%d",&A);
    scanf("%d",&B);
    scanf("%d",&C);
    scanf("%d",&D);
    
    if(B>C && D>A && C+D > A+B && C && D >0 && A%2==0);
    
    {
     printf("Valores aceitos\n");   
    }
    
    if(!(B>C && D>A && C+D > A+B && C && D >0 && A%2==0));
     
     {
         printf("Valores n√£o aceitos\n");
     }
    
    return 0;
}
