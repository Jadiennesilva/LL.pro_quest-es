#include <stdio.h>
 
int main() {
    
    double A,B,C;
    
    
    scanf("%lf",&A);
    scanf("%lf",&B);
    
   C = (A*3.5+B*7.5) / 11;
    
    printf("MEDIA = %lf,%lf\n",C);
 
 
 
    return 0;
}

 
int main() {
 
    double A, B, media;
    
    scanf("%lf",&A);
    scanf("%lf",&B)
    
    media = ((A*3.5) + (B*7.5))/11;
    printf("MEDIA = %lf\n", media);
    return 0;
}

